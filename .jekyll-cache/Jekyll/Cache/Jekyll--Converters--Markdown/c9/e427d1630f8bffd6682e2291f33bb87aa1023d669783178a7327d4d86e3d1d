		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"--4-2. 등록하기(실패!! - kind 잘못!!!!)"</span><span class="o">);</span>
		<span class="nc">BoardController</span><span class="o">.</span><span class="na">insertBoard</span><span class="o">(</span><span class="s">"photo2"</span> <span class="o">,</span>
				<span class="k">new</span> <span class="nf">PhotoBoard</span><span class="o">(</span><span class="mi">50</span><span class="o">,</span> <span class="s">"mvc공부"</span><span class="o">,</span> <span class="s">"장희정"</span><span class="o">,</span> <span class="s">"재미있다."</span><span class="o">,</span> <span class="s">"2018-08-18"</span><span class="o">,</span> <span class="s">"a.jpg"</span><span class="o">));</span>
		
		
