		<span class="nc">BoardController</span><span class="o">.</span><span class="na">getBoardByNo</span><span class="o">(</span><span class="s">"archive"</span><span class="o">,</span> <span class="mi">50</span><span class="o">);</span><span class="c1">// 없는 게시물</span>
		
         <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">();</span>
		
